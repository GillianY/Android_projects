<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <alpha
        android:fromAlpha="1"
        android:toAlpha="0"
        android:duration="800"/>
    <scale
        android:pivotX="50%"
        android:pivotY="50%"
        android:fromXScale="1"
        android:toXScale="0"
        android:fromYScale="1"
        android:toYScale="0"
        android:duration="800"/>
    <translate
        android:pivotX="50%"
        android:pivotY="50%"
        android:fromXDelta="1"
        android:toXDelta="0"
        android:fromYDelta="1"
        android:toYDelta="0"
        android:duration="800"
        />
    <rotate
        android:pivotX="50%"
        android:pivotY="50%"
        android:fromDegrees="720"
        android:toDegrees="0"
        android:duration="800"
        />
</set>